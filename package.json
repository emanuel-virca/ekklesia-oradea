{
  "name": "ekklesia-oradea",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=development",
    "docker-start": "ng serve --configuration=development --host 0.0.0.0",
    "build-dev": "ng build --configuration=development",
    "build-test": "ng build --configuration=testing --prod",
    "build-prod": "ng build --configuration=production --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "firebase-dev": "firebase use ekklesia-oradea-dev",
    "firebase-test": "firebase use ekklesia-oradea-test",
    "firebase-prod": "firebase use ekklesia-oradea-prod",
    "deploy-dev": "npm run firebase-dev && firebase deploy --except hosting",
    "deploy-test": "npm run build-test && npm run firebase-test && firebase deploy",
    "deploy-prod": "npm run build-prod && npm run firebase-prod && firebase deploy",
    "deploy-hosting-test": "npm run build-test && npm run firebase-test && firebase deploy --only hosting",
    "deploy-hosting-prod": "npm run build-prod && npm run firebase-prod && firebase deploy --only hosting",
    "deploy-functions-dev": "npm run firebase-dev && firebase deploy --only functions",
    "deploy-functions-test": "npm run firebase-test && firebase deploy --only functions",
    "deploy-functions-prod": "npm run firebase-prod && firebase deploy --only functions",
    "stats": "webpack-bundle-analyzer dist/ekklesia-oradea/stats.json",
    "build-stats": "ng build --configuration=production --prod --stats-json && npm run stats"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "{src,e2e,cypress}/**/*.{ts,json,md,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/fire": "^5.3.0",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "~8.2.14",
    "@angular/material": "~8.1.4",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/pwa": "^0.803.24",
    "@angular/router": "~8.2.14",
    "@angular/service-worker": "~8.2.14",
    "@ngneat/until-destroy": "^7.1.0",
    "@ngrx/effects": "^8.6.0",
    "@ngrx/schematics": "^8.6.0",
    "@ngrx/store": "^8.6.0",
    "@ngrx/store-devtools": "^8.6.0",
    "algoliasearch": "~3.35.1",
    "firebase": "^7.5.2",
    "ng-oidc-client": "^1.0.7",
    "ngx-infinite-scroll": "~8.0.1",
    "ngx-masonry": "~1.1.4",
    "oidc-client": "^1.10.1",
    "rxjs": "^6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.24",
    "@angular/cli": "^8.3.24",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "~8.2.0",
    "@types/algoliasearch": "^3.34.10",
    "@types/jasmine": "^3.5.2",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.12.26",
    "codelyzer": "^5.2.1",
    "firebase-tools": "^7.12.1",
    "husky": "~3.1.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.1",
    "lint-staged": "^9.5.0",
    "prettier": "~1.19.1",
    "protractor": "^5.4.3",
    "standard-version": "~7.0.1",
    "ts-node": "^8.5.4",
    "tslint": "~5.20.1",
    "typescript": "^3.7.5",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
